  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "eslint": "^1.6.0",
    "jest-cli": "^0.5.10"
  },
  "scripts": {
    "test": "BABEL_JEST_STAGE=0 jest ./src",
    "test:acceptance": "BABEL_JEST_STAGE=0 jest --config=./acceptanceTestConfig.json",

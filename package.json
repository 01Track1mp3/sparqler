  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathDirs": [
      "src/"
    ],
    "unmockedModulePathPatterns": [
      "lodash"
    ]
  },
  "gitHead": "631b42013196b6b841886c2ab8c536091d1d4693",
